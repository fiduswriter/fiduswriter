{
  "name": "fidus-writer",
  "version": "3.2.0-dev",
  "description": "Install dependencies for ES6 transpilation",
  "scripts": {
    "get_mml2omml": "mkdir -p ./static-libs/xsl/ && wget https://raw.githubusercontent.com/AbiWord/abiword/14cbf144cb10a5a5ea6ee11275f7178ca4ff055a/plugins/openxml/omml_xslt/mml2omml.xsl -O ./static-libs/xsl/mml2omml.xsl",
    "copy_jquery": "mkdir -p ./static-libs/js/libs/ && cp node_modules/jquery/dist/jquery.min.js ./static-libs/js/libs/",
    "copy_jquery-ui": "mkdir -p ./static-libs/js/libs/ && cp node_modules/jquery-ui-dist/jquery-ui.min.js ./static-libs/js/libs/",
    "copy_underscore": "mkdir -p ./static-libs/js/libs/ && cp node_modules/underscore/underscore-min.js ./static-libs/js/libs/",
    "copy_datatables": "mkdir -p ./static-libs/js/libs/ && cp node_modules/datatables/media/js/jquery.dataTables.min.js ./static-libs/js/libs/",
    "copy_dropkickjs": "mkdir -p ./static-libs/js/libs/ && cp node_modules/dropkickjs/build/js/dropkick.min.js ./static-libs/js/libs/",
    "copy_dropkickjs_css": "mkdir -p ./static-libs/css/libs/ && cp node_modules/dropkickjs/build/css/dropkick.css ./static-libs/css/libs/",
    "copy_citeproc": "mkdir -p ./static-libs/js/libs/ && cp node_modules/citeproc/citeproc.js ./static-libs/js/libs/",
    "copy_texlive": "mkdir -p ./static-libs/js/libs/texlive && cp -r node_modules/texlive/* ./static-libs/js/libs/texlive",
    "copy_texlive_req": "mkdir -p ./static-libs/js/libs/texlive && cp -r style/static/export_requirements/* ./static-libs/js/libs/texlive",
    "copy_promisejs": "mkdir -p ./static-libs/js/libs/texlive/promisejs && cp -r node_modules/promisejs/* ./static-libs/js/libs/texlive/promisejs",
    "postinstall": "npm run get_mml2omml && npm run copy_jquery && npm run copy_jquery-ui && npm run copy_underscore && npm run copy_datatables && npm run copy_dropkickjs && npm run copy_dropkickjs_css && npm run copy_citeproc && npm run copy_texlive && npm run copy_promisejs && npm run copy_texlive_req",
    "check_js": "jshint .",
    "check_py": "flake8 ./",
    "install": "napa"
  },
  "author": "Johannes Wilm",
  "license": "AGPL",
  "private": true,
  "dependencies": {
    "babel-preset-es2015": "6.16.0",
    "babelify": "7.3.0",
    "biblatex-csl-converter": "0.21.4",
    "bowser": "1.4.6",
    "browserify": "13.1.0",
    "browserify-incremental": "3.1.1",
    "browserkeymap": "^1.0.0",
    "datatables": "1.10.12",
    "downloadjs": "^1.4.6",
    "dropkickjs": "2.1.10",
    "fg-loadcss": "1.2.0",
    "jquery": "3.1.1",
    "jquery-ui-dist": "1.12.1",
    "jshint": "2.9.3",
    "jszip": "3.1.2",
    "jszip-utils": "0.0.2",
    "katex": "0.6.0",
    "mathquill": "0.10.1-b",
    "napa": "2.3.0",
    "object-hash": "1.1.4",
    "paginate-for-print": "0.0.6",
    "prosemirror-old": "0.10.4",
    "texzilla": "0.9.9",
    "tokenfield": "^0.3.14",
    "underscore": "1.8.3"
  },
  "napa": {
    "citeproc": "Juris-M/citeproc-js#1.1.142",
    "texlive" : "fiduswriter/texlive.js#master",
    "promisejs":"stackp/promisejs#master"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "compact": false
        }
      ]
    ]
  },
  "jshintConfig": {
    "esversion": 6,
    "loopfunc": true,
    "asi": true,
    "multistr": true,
    "sub": true,
    "undef": true,
    "predef": [
      "window",
      "document",
      "console",
      "jQuery",
      "MathQuill",
      "CSL",
      "gettext",
      "_"
    ]
  }
}
