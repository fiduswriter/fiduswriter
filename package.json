{
  "name": "fidus-writer",
  "version": "3.0.0-beta.2",
  "description": "Install dependencies for ES6 transpilation",
  "scripts": {
  },
  "author": "Johannes Wilm",
  "license": "AGPL",
  "private": true,
  "dependencies": {
    "browserify-incremental": "3.1.1",
    "browserify": "13.0.1",
    "babelify": "7.3.0",
    "babel-preset-es2015": "6.6.0",
    "prosemirror": "git://github.com/fiduswriter/prosemirror.git#e8d8d719e2b700ab75f6b8c786c3c2f95ee8f9e0",
    "katex": "0.6.0",
    "object-hash": "1.1.2",
    "mathquill": "0.10.1-b"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
        ],
          "compact": false
        }
      ]
    ]
  },
  "jshintConfig": {
    "esversion": 6,
    "loopfunc": true,
    "asi": true,
    "multistr": true,
    "sub": true,
    "undef": true,
    "predef": [
      "jQuery",
	  "gettext",
	  "_",
	  "window",
	  "$",
	  "document",
	  "console",
	  "FileReader",
	  "FormData",
	  "alert",
	  "setTimeout",
	  "zip",
      "setInterval",
      "clearInterval",
      "clearTimeout",
      "CSL",
      "XMLHttpRequest"
    ]
  }
}
